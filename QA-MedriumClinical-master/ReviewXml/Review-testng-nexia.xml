<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Nexia 1.7 Auto Review Only testcases - US Account @10.33.2.159">
 
  <parameter name="webSite" value="https://nexia-qa3.nexia.local:8443/" />
  <parameter name="seleniumHost" value="10.33.2.159" />
  <parameter name="seleniumPort" value="4444" />
  <parameter name="browser" value="*firefox" />
  <parameter name="userAccount" value="US2000" />
  
  <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  </listeners>
    
  <test name="Nexia ReviewOnly" parallel="methods" thread-count="7">
  
  <groups>		      
	<run>
	   <include name="Review"/>	   
	</run>
  </groups>  
  	   
  <packages> 
  
  <!-- Smoke Testcases -->
  <package name="com.nexia.selenium.testscripts.section.smoketest.*"/>  
  <!-- Demographics Testcases -->
  <package name="com.nexia.selenium.testscripts.section.demographics.*"/>  
  <!-- Operation  Testcases -->  
 <!-- <package name="com.nexia.selenium.testscripts.section.operations.*"/> -->  
  <!-- Clincal List, MU and CQM  Testcases -->
  <package name="com.nexia.selenium.testscripts.section.clinicalLists.*"/>
  <package name="com.nexia.selenium.testscripts.section.clinicalsettings.*"/>
  <!-- Charprevisit Testcases -->
   <package name="com.nexia.selenium.testscripts.section.chartprevisit.*"/>
   <!-- taskManagment.PatientTask Testcases -->
   <package name="com.nexia.selenium.testscripts.section.taskManagment.PatientTask.*"/>
   <!-- TemplateSetup daySheet Template -->
   <package name="com.nexia.selenium.testscripts.section.templateSetup.daySheetTemplate.*"/>
   <package name="com.nexia.selenium.testscripts.section.systemsettings.*"/>
   <!-- Billing TestCases -->
   <package name="com.nexia.selenium.testscripts.section.billing.*"/>
   <!-- Scheduling Settings -->
   <package name="com.nexia.selenium.testscripts.section.schedulingsettings.*"/>      
   
  </packages>
  </test>    
</suite>