   <parameter name="seleniumHost" value="10.1.13.92" />
   <parameter name="seleniumPort" value="4444" />
    
  <groups>  
		   <define name="CA">    
		      <include name="RegressionCA"/>           
		    </define>
		    
		     <define name="Common">    
		      <include name="Regression"/>           
		    </define>
		  
		    <define name="CanadaAccount">
		      <include name="CA"/>      
		      <include name="Common"/>
		    </define>
		      
		    <run>
		    	 <include name="CanadaAccount"/>
		    </run>
	  </groups>
   
    <!-- com.nexia.selenium.testscripts.section.taskManagment.PatientTask -->
    <packages>
      <package name="com.nexia.selenium.testscripts.section.taskManagment.patient_Task.*"/>
      </packages>
   </test>   

  
         
   
</suite>